<%= form_with(model: @group, local: true) do |f| %>
<%= render 'layouts/error_messages', model: f.object %>
    <div class="form-group">
        <%= f.label :name, 'グループ名' %>
        <%= f.text_field :name, class: 'form-control'  %>
    </div>
        <%= f.submit '登録', class: 'btn btn-primary btn-block'  %>
<% end %>
